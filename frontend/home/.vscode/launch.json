{
  "configurations": [
    {
      "name": "Launch React Home",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3001",
      "webRoot": "${workspaceFolder}/src",
      // "preLaunchTask": "Start/frontend/home",
      "server": {
        "command": "npm start"
      },
      "preLaunchTask": "Start/frontend/home",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
