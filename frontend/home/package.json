{
  "name": "home-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "docker:build": "docker build --force-rm --tag app-home:latest .",
    "docker:run": "docker run --network networkOne -l 'container:home-react' --name home-react -d --rm -p 5000:80 app-home:latest",
    "docker:run:local": "docker run --network networkOne -l 'local:home-react' --name home-react -d --cpus 4 --rm -p 5000:80 app-home:latest",
    "docker:run:live": "docker run --network networkOne -l 'live:home-react' --name home-react -it --cpus 4 --rm -p 5000:80 app-home:latest",
    "docker:build:run": "npm run docker:build && npm run docker:run",
    "docker:build:run:local": "npm run docker:build && npm run docker:run:local",
    "docker:build:run:live": "npm run docker:build && npm run docker:run:live",
    "docker:stop": "docker stop home-react",
    "build:local": "react-scripts build; yarn analyze",
    "build": "echo skip",
    "build:prod": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@babel/core": "~7.19.3",
    "@babel/plugin-syntax-flow": "~7.18.6",
    "@babel/plugin-transform-react-jsx": "~7.19.0",
    "@testing-library/dom": "~8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "source-map-explorer": "~2.5.3",
    "web-vitals": "~3.0.3",
    "tslib": "2.4.0",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "6.11.0",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/node": "16",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "~5.40.1",
    "@typescript-eslint/parser": "~5.40.1",
    "eslint": "~8.25.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-import-resolver-typescript": "~3.5.1",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsdoc": "~39.3.13",
    "eslint-plugin-jsonc": "~2.4.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react": "~7.31.10",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-simple-import-sort": "~8.0.0",
    "eslint-plugin-unicorn": "~44.0.2",
    "nodemon": "~2.0.20",
    "prettier": "~2.7.1",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.4",
    "@rushstack/heft": "~0.48.6",
    "@rushstack/heft-jest-plugin": "~0.3.43",
    "@types/heft-jest": "1.0.3",
    "jest": "~29.2.0",
    "@types/jest": "29.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
