{
  "name": "api-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "docker:build": "docker build --label image:api-server --progress plain --tag server-api:latest .",
    "docker:run": "docker run --network networkOne -l 'container:api-server' --name api-server -d --rm -p 3000:80 server-api:latest",
    "docker:run:local": "docker run --network networkOne -l 'local:api-server' --name api-server -d --rm --cpus 4 -p 3000:80 server-api:latest",
    "docker:run:live": "docker run --network networkOne -l 'live:api-server' --name api-server -t -i --rm --cpus 4 -p 3000:80 server-api:latest",
    "docker:build:run": "npm run docker:build && npm run docker:run",
    "docker:build:run:local": "npm run docker:build && npm run docker:run:local",
    "docker:build:run:live": "npm run docker:build && npm run docker:run:live",
    "docker:rerun": "docker stop api-server && npm run docker:run",
    "docker:rerun:local": "docker stop api-server && npm run docker:run:local",
    "docker:rerun:live": "docker stop api-server && npm run docker:run:live",
    "docker:stop": "docker stop api-server",
    "build:prod": "nest build --tsc",
    "deploy": "rimraf dist; nest build --tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "~9.4.0",
    "@nestjs/core": "~9.4.0",
    "@nestjs/platform-express": "~9.4.0",
    "class-transformer": "~0.5.1",
    "class-validator": "~0.14.0",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~5.0.0",
    "rxjs": "~7.8.1",
    "@luxcium/redis-services": "workspace:*",
    "@luxcium/phash-compute": "workspace:*",
    "@luxcium/scan-dirs": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "~7.22.5",
    "@babel/plugin-transform-typescript": "~7.22.5",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "~9.1.0",
    "@nestjs/testing": "^9.0.0",
    "@rushstack/eslint-config": "~3.3.2",
    "@rushstack/heft-jest-plugin": "~0.7.11",
    "@rushstack/heft": "~0.56.0",
    "@types/chai": "~4.3.5",
    "@types/eslint-config-prettier": "~6.11.0",
    "@types/eslint-plugin-prettier": "~3.1.0",
    "@types/express": "~4.17.17",
    "@types/fs-extra": "~11.0.1",
    "@types/glob": "~8.1.0",
    "@types/heft-jest": "~1.0.3",
    "@types/mime-types": "~2.1.1",
    "@types/node": "18",
    "@types/prettier": "~2.7.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "~5.60.1",
    "@typescript-eslint/parser": "~5.60.1",
    "chai": "~4.3.7",
    "concurrently": "~8.0.1",
    "eslint-config-prettier": "~8.8.0",
    "eslint-import-resolver-typescript": "~3.5.5",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jsdoc": "~46.4.3",
    "eslint-plugin-jsonc": "~2.9.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "eslint-plugin-tsdoc": "~0.2.17",
    "eslint-plugin-unicorn": "~47.0.0",
    "eslint": "~8.44.0",
    "jest": "~29.5.0",
    "nodemon": "~2.0.22",
    "prettier": "~2.8.8",
    "rimraf": "~5.0.0",
    "source-map-support": "~0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "~29.1.1",
    "ts-loader": "~9.4.4",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.2.0",
    "typescript": "~5.1.6",
    "webpack": "~5.88.1",
    "@jest/globals": "~29.5.0",
    "@types/eslint": "~8.40.2",
    "eslint-config-next": "~13.4.7",
    "eslint-config-react-app": "~7.0.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-webpack-plugin": "~4.0.1",
    "@babel/plugin-syntax-flow": "~7.22.5",
    "@babel/plugin-transform-react-jsx": "~7.22.5",
    "tslib": "~2.6.0",
    "@types/jest": "~29.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
